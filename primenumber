# prime number



def prime():
    num = int(input("Enter the number you want to check\n"))
    count = 0
    for i in range(1, num+1 //2):
        if num % i == 0:
            count +=1
        else:
            continue

    if count == 1:
        print("prime")
    else:
        print("not a prime")


prime()
